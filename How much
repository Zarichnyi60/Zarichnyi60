package com.company;

import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        // Here is a code which does some kind of magic.

        int a;
        int b;
        int c;

        Scanner scanner = new Scanner(System.in);
        System.out.println("Введите число а");
        a = scanner.nextInt();
        System.out.println("Введите число b");
        b = scanner.nextInt();
        System.out.println("Введите число с");
        c = scanner.nextInt();

        if (a > b && a > c)
            System.out.println("Больше всех a = " + a);
        else if (b > a && b > c)
            System.out.println("Больше всех b = " + b);
        else if (c > a && c > b)
            System.out.println("Больше всех c = " + c);
        else if (a == b && b == c && a == c)
            System.out.println("Все числа равны");
        else if(a == b)
            System.out.println("Больше всех a&&b = " + a);
        else if(a == c)
            System.out.println("Больше всех a&&c = " + c);
        else if(b == c)
            System.out.println("Больше всех b&&c" + b);


    }
}
